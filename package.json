  },
  "homepage": "https://github.com/Thaisrr/abra.js#readme",
  "dependencies": {
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@testdeck/mocha": "^0.3.3",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node-fetch": "^2.6.2",
    "chai": "^4.3.7",
    "json-server": "^0.17.1",
    "json-server-auth": "^2.1.0",
    "mocha": "^10.2.0",
    "mocha-typescript": "^1.1.17",
